    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "@blueprintjs/core": "^1.12.0",
    "babel-cli": "^6.23.0",
    "express": "^4.15.0",
    "express-ws": "^3.0.0",
    "lz-string": "^1.4.4",
